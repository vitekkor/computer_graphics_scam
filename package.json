{
  "name": "computer_graphics_scam",
  "version": "2.0.0",
  "description": "todo",
  "main": "main.js",
  "author": "vitekkor",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ -all",
    "build": "electron-builder",
    "release": "electron-builder --publish always"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "follow-redirects": "^1.14.4",
    "adm-zip": "^0.5.9"
  },
  "devDependencies": {
    "electron": "^15.0.0",
    "electron-packager": "^16.0.0",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "linux": {
      "target": [
        {
          "target": "AppImage"
        },
        {
          "target": "tar.gz"
        }
      ],
      "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
      "icon": "icons/icon.png",
      "category": "Development"
    },
    "win": {
      "target": [
        {
          "target": "portable"
        },
        {
          "target": "zip"
        }
      ],
      "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
      "icon": "icons/icon.png"
    },
    "publish": {
      "provider": "github",
      "repo": "computer_graphics_scam",
      "owner": "vitekkor",
      "releaseType": "release"
    }
  }
}
